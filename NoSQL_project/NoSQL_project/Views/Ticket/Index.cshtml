@model List<MongoDB.Bson.BsonDocument>
<h2>Tickets by Status</h2>
<table class="table">
    <thead><tr><th>Status</th><th>Count</th><th>%</th></tr></thead>
    <tbody>
        @foreach (var d in Model)
        {
            <tr>
                <td>@d.GetValue("status", MongoDB.Bson.BsonNull.Value)</td>
                <td>@d.GetValue("count", MongoDB.Bson.BsonNull.Value)</td>
                <td>@d.GetValue("percentage", MongoDB.Bson.BsonNull.Value)</td>
            </tr>
        }
    </tbody>
</table>
